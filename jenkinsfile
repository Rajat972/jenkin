pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        sh '''pwd
date'''
      }
    }

    stage('test') {
      parallel {
        stage('test') {
          steps {
            echo 'test mesasage'
            sleep 10
          }
        }

        stage('test parallel') {
          steps {
            sh 'date'
            sleep 5
          }
        }

      }
    }

    stage('prod') {
      agent any
      steps {
        echo 'enter to prod'
        sleep 5
      }
    }

  }
}
